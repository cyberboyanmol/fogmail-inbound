    "@types/lodash": "^4.17.7",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.3.1",
    "@types/shelljs": "^0.8.15",
    "@types/smtp-server": "^3.5.10",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
